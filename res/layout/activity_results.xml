<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical" android:layout_width="match_parent"
    android:layout_height="match_parent"
    >

    <Button
        android:layout_width="75dp"
        android:layout_height="wrap_content"
        android:background="@drawable/custom_button"
        android:text="Q1"
        android:textSize="20dp"
        android:fontFamily="sans-serif-thin"
        android:id="@+id/q1"
        android:layout_alignParentTop="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_marginTop="30dp"
        android:layout_marginLeft="20dp" />

    <Button
        android:layout_width="75dp"
        android:layout_height="wrap_content"
        android:background="@drawable/custom_button"
        android:text="Q2"
        android:textSize="20dp"
        android:fontFamily="sans-serif-thin"
        android:id="@+id/q2"
        android:layout_alignBottom="@+id/q1"
        android:layout_centerHorizontal="true" />

    <Button
        android:layout_width="75dp"
        android:layout_height="wrap_content"
        android:background="@drawable/custom_button"
        android:text="Q3"
        android:textSize="20dp"
        android:fontFamily="sans-serif-thin"
        android:id="@+id/q3"
        android:layout_alignBottom="@+id/q2"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true"
        android:layout_marginRight="20dp" />

    <Button
        android:layout_width="75dp"
        android:layout_height="wrap_content"
        android:background="@drawable/custom_button"
        android:text="q4"
        android:textSize="20dp"
        android:fontFamily="sans-serif-thin"
        android:id="@+id/q4"
        android:layout_marginTop="33dp"
        android:layout_below="@+id/q1"
        android:layout_alignLeft="@+id/q1"
        android:layout_alignStart="@+id/q1" />

    <Button
        android:layout_width="75dp"
        android:layout_height="wrap_content"
        android:background="@drawable/custom_button"
        android:text="Q5"
        android:textSize="20dp"
        android:fontFamily="sans-serif-thin"
        android:id="@+id/q5"
        android:layout_alignBottom="@+id/q4"
        android:layout_centerHorizontal="true" />

    <Button
        android:layout_width="75dp"
        android:layout_height="wrap_content"
        android:background="@drawable/custom_button"
        android:text="Q6"
        android:textSize="20dp"
        android:fontFamily="sans-serif-thin"
        android:id="@+id/q6"
        android:layout_alignBottom="@+id/q5"
        android:layout_alignLeft="@+id/q3"
        android:layout_alignStart="@+id/q3" />

    <Button
        android:layout_width="75dp"
        android:layout_height="wrap_content"
        android:background="@drawable/custom_button"
        android:text="Q7"
        android:textSize="20dp"
        android:fontFamily="sans-serif-thin"
        android:id="@+id/q7"
        android:layout_below="@+id/q4"
        android:layout_alignLeft="@+id/q4"
        android:layout_alignStart="@+id/q4"
        android:layout_marginTop="31dp" />

    <Button
        android:layout_width="75dp"
        android:layout_height="wrap_content"
        android:background="@drawable/custom_button"
        android:text="Q8"
        android:textSize="20dp"
        android:fontFamily="sans-serif-thin"
        android:id="@+id/q8"
        android:layout_alignBottom="@+id/q7"
        android:layout_centerHorizontal="true" />

    <Button
        android:layout_width="75dp"
        android:layout_height="wrap_content"
        android:background="@drawable/custom_button"
        android:text="Q9"
        android:textSize="20dp"
        android:fontFamily="sans-serif-thin"
        android:id="@+id/q9"
        android:layout_alignBottom="@+id/q8"
        android:layout_alignLeft="@+id/q6"
        android:layout_alignStart="@+id/q6" />

    <Button
        android:layout_width="75dp"
        android:layout_height="wrap_content"
        android:background="@drawable/custom_button"
        android:text="Q10"
        android:textSize="20dp"
        android:fontFamily="sans-serif-thin"
        android:id="@+id/q10"
        android:layout_marginTop="35dp"
        android:layout_below="@+id/q7"
        android:layout_alignLeft="@+id/q7"
        android:layout_alignStart="@+id/q7" />

    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/finish"
        android:src="@drawable/arrow_12_32"
        android:background="@null"
        android:layout_below="@+id/q10"
        android:layout_alignRight="@+id/q9"
        android:layout_alignEnd="@+id/q9"
        android:layout_marginTop="45dp"
        android:onClick="backToHome" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="Finish"
        android:id="@+id/textView7"
        android:layout_below="@+id/finish"
        android:layout_alignLeft="@+id/finish"
        android:layout_alignStart="@+id/finish" />

</RelativeLayout>